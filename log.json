{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "basic_format": {
      "format": "%(asctime)s.%(msecs)03d %(levelname)-6s %(message)s",
      "datefmt": "%Y-%m-%dT%H:%M:%S"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "DEBUG",
      "formatter": "basic_format",
      "stream": "ext://sys.stdout"
    },
    "file": {
      "class": "logging.handlers.RotatingFileHandler",
      "level": "DEBUG",
      "formatter": "basic_format",
      "filename": "fantasy_console.log",
      "maxBytes": 1073741824,
      "backupCount": 5,
      "encoding": "utf8"
    }
  },
  "loggers": {
    "fantasy_console": {
      "level": "DEBUG",
      "handlers": [
        "console",
        "file"
      ]
    }
  },
  "root": {
    "level": "ERROR",
    "handlers": []
  }
}